{
    "name": "danielkorytek/messenger-bridge-bundle",
    "license": "proprietary",
    "type": "symfony-bundle",
    "description": "Messenger bridge for Docplanner messaging standard",
    "require": {
        "php": "^7.2.5 || ^8.0",
        "symfony/messenger": "^4.4 || ^5.0 || ^6.0 || ^7.0",
        "symfony/http-kernel": "*",
        "symfony/dependency-injection": "*",
        "symfony/serializer": "*"
    },
    "authors": [
        {
            "name": "Daniel Korytek",
            "email": "daniel.korytek@docplanner.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "DanielKorytek\\MessengerBridgeBundle\\": "src"
        }
    },
    "suggest": {
        "symfony/amqp-messenger": "Required since symfony/messenger:^5.1"
    }
}
